camera EXTERNAL(HOST:"localhost", PORT:5600, tab:Test);

Test MACHINE camera {

    test WHEN TIMER > 5000;
    idle DEFAULT;

    ENTER test { SEND CapturePhoto TO camera; SET SELF TO idle; }
    RECEIVE TestMessage {
        LOG "Got test message";
    }
}
test Test(tab:Test) camera;

ROUTE "TestMessage" TO test;

arduino EXTERNAL(DEVICE:"/dev/tty.usbserial-A900ced4", BAUD:9600, 
    BITS:8, PARITY:"NONE", STOPBITS:1);

