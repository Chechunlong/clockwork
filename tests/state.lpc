Test MACHINE {
	a STATE;
	b STATE;
	c STATE;

	x WHEN last == "b";
	none DEFAULT;

ENTER c { last:="c"; LOG "c" }
ENTER b { last:="b"; LOG "b" }
ENTER a { last:="a"; LOG "a" }

COMMAND C {}
COMMAND B {}
COMMAND A {}

TRANSITION INIT,a,b TO c USING C;
TRANSITION INIT,a,c TO b USING B;
TRANSITION INIT,b,c TO a USING A;
}
test Test (last:"",tab:Tests);
